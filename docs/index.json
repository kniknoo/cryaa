cii:String?)-instance-method","name":"ascii=","doc":null,"summary":null,"abstract":false,"args":[{"name":"ascii","doc":null,"default_value":"","external_name":"ascii","restriction":"String | Nil"}],"args_string":"(ascii : String?)","source_link":null,"def":{"name":"ascii=","args":[{"name":"ascii","doc":null,"default_value":"","external_name":"ascii","restriction":"String | Nil"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"@ascii = ascii"}},{"id":"asciify-instance-method","html_id":"asciify-instance-method","name":"asciify","doc":null,"summary":null,"abstract":false,"args":[],"args_string":"","source_link":null,"def":{"name":"asciify","args":[],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"line = [] of String\n@height.times do |y|\n  y2 = y + @box_h\n  if y2 > @image.height\n    return line.join\n  end\n  @width.times do |x|\n    x2 = x + @box_w\n    if box_start?(x, y)\n      if x2 > @width\n        line << \"\\n\"\n        next\n      else\n        line << (character_index(grey_of_box(x, y))).to_s\n      end\n    end\n  end\nend\n"}},{"id":"box_h:UInt8-instance-method","html_id":"box_h:UInt8-instance-method","name":"box_h","doc":null,"summary":null,"abstract":false,"args":[],"args_string":" : UInt8","source_link":null,"def":{"name":"box_h","args":[],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"UInt8","visibility":"Public","body":"@box_h"}},{"id":"box_h=(box_h:UInt8)-instance-method","html_id":"box_h=(box_h:UInt8)-instance-method","name":"box_h=","doc":null,"summary":null,"abstract":false,"args":[{"name":"box_h","doc":null,"default_value":"","external_name":"box_h","restriction":"UInt8"}],"args_string":"(box_h : UInt8)","source_link":null,"def":{"name":"box_h=","args":[{"name":"box_h","doc":null,"default_value":"","external_name":"box_h","restriction":"UInt8"}],"double_splat":null,"sp